{
  "name": "chroma",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8008",
    "build": "vue-cli-service build",
    "protos": "find ../server/proto/protos -type f -name \\*.proto -exec protoc -I ../server/proto/protos --ts_opt=json_names --ts_opt=esModuleInterop=true --ts_opt=no_namespace --ts_out=./src/generated {} \\;"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.15.6",
    "core-js": "^3.8.3",
    "google-protobuf": "^3.21.2",
    "protoc-gen-ts": "^0.8.5",
    "vue": "^2.6.14",
    "vue-router": "^3.5.1",
    "vuetify": "^2.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "typescript": "~4.5.5",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  }
}
